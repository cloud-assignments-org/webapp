{
  "name": "webapp-fork",
  "version": "1.0.0",
  "description": "repository for web applications",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npx jest",
    "dev": "nodemon --exec npx esrun src/index.ts",
    "clean-up" : "rm -rf dist && rm -rf ./src/routes",
    "generate-tsoa": "yarn run tsoa spec-and-routes",
    "pre-build" : "node preProcess.js",
    "build" : "npm run clean-up && npm run generate-tsoa && npm run test && npm run pre-build && tsc",
    "serve" : "node dist/src/index.js",
    "demo" : "npm run build && npm run serve"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.0",
    "@types/swagger-ui-express": "^4.1.6",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@tsoa/runtime": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0"
  }
}
